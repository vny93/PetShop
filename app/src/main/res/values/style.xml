<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="style_imvlogin">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/_120sdp</item>
        <item name="android:src">@drawable/logo</item>
    </style>

    <style name="style_cardview_userlogin">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="cardCornerRadius">@dimen/_8sdp</item>
    </style>

    <style name="style_cardview_itemProductType">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="cardCornerRadius">@dimen/_8sdp</item>
    </style>

    <style name="style_cardview_panner">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="cardCornerRadius">@dimen/_5sdp</item>
        <item name="android:layout_marginStart">@dimen/_10sdp</item>
        <item name="android:layout_marginEnd">@dimen/_10sdp</item>
        <item name="android:layout_marginTop">@dimen/_10sdp</item>
    </style>

    <style name="style_textview_login">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/_14sdp</item>
        <item name="android:layout_marginStart">@dimen/_33sdp</item>
    </style>

    <style name="style_textview_login2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/_13sdp</item>
    </style>


    <style name="style_textview_forgotpw">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/_13sdp</item>
        <item name="android:textStyle">italic</item>
        <item name="android:layout_marginStart">@dimen/_17sdp</item>
    </style>

    <style name="style_btnlogin">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/custom_button</item>
        <item name="android:padding">@dimen/_12sdp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="style_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/_14sdp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="style_textview2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="style_edit_login">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:hint">Enter you username</item>
        <item name="android:textSize">@dimen/_13sdp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginStart">@dimen/_30sdp</item>
        <item name="android:layout_marginEnd">@dimen/_30sdp</item>
    </style>

    <style name="style_itemHistory2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/_9sdp</item>
    </style>

    <style name="style_itemHistory3">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/_11sdp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="style_textview_normal">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/_15sdp</item>
    </style>
    <style name="style_textview_profile">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/_13sdp</item>
    </style>

</resources>